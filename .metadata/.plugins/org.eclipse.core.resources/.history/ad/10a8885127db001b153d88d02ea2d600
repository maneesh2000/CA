import java.util.Scanner;

public class PatternPrograme {

	public static void main(String[] args) {
	Scanner scan=new Scanner(System.in);
		
		System.out.println("Enter your limit:");
		int limit=scan.nextInt();
		
		for(int i=1;i<limit;i++) {
			
			
			if(i%2==0) {
				
				System.out.println("* *");
			
				
				
				
			}else {
				
				
				System.out.println("*");
				
				
				
			}
			
			
			
			
			
		}

	}

}
